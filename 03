// Online C++ compiler to run C++ program online
#include <iostream>
#include<string>
using namespace std;
class student{
    public:
    string name;
    int roll_no;
    student();
void output();
~student(){
    cout<<endl<<"Destructor is called";
}
};
 student ::student(){
    cout<<"Enter name : ";
    cin>>name;
    cout<<"Enter Roll no : ";
    cin>>roll_no;
}
void student ::output(){
    cout<<"Entered name : "<<name;
    cout<<endl;
    cout<<"Entered Roll no : "<<roll_no;
}
int main() {
student s1;
s1.output();
    return 0;
}
